{
    "author": "John Murphy",
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "description": "The Smile Redirect extension automatically redirects from amazon.com to smile.amazon.com",
    "icons": {
        "128": "icons/smile-icon-128.png"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'; style-src 'self' 'unsafe-inline';",
    "manifest_version": 2,
    "name": "Smile Redirect",
    "permissions": [
        "tabs",
        "background",
        "webNavigation"
    ],
    "version": "1.0.0",
    "web_accessible_resources": [
        "icons/*.png",
        "template.js",
        "background.js"
    ]
}